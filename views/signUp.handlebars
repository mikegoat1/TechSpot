<div class="container-fluid">
        <div class="row">

            <div class="collogin-card">
                <h2 class="page-title">Sign Up!</h2>

                <form class="login-form">
                    <div class="mb-3">
                        <label for="email-signup" class="form-label">Email address</label>
                        <input type="email" class="form-control" id="email-signup" aria-describedby="emailHelp">
                    </div>
                    <div class="mb-3">
                        <label for="password-signup" class="form-label">Password</label>
                        <input type="password" class="form-control" id="password-signup">
                    </div>
                    <a href="/login" id="goBack" type="submit" class="btn">Go Back</a>

                    <a id="signUpBtn" type="submit" class="btn">Sign Up</a>
                </form>

            </div>

                 <script src="/js/signIn.js"></script>